#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_autoprefixer@7.2.5@autoprefixer/bin/autoprefixer-info" "$@"
  ret=$?
else 
  node  "$basedir/../_autoprefixer@7.2.5@autoprefixer/bin/autoprefixer-info" "$@"
  ret=$?
fi
exit $ret
