#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_win-fork@1.1.1@win-fork/bin/win-line-endings" "$@"
  ret=$?
else 
  node  "$basedir/../../../_win-fork@1.1.1@win-fork/bin/win-line-endings" "$@"
  ret=$?
fi
exit $ret
